<h1> Log In</h1>




    <%= form_for(:session, url: login_path) do |f| %>
      <p>
        <%= f.label :email %>
        <%= f.email_field :email, placeholder: "Enter email" %>
      </p>
      <p>
        <%= f.label :password %>
        <br>
        <%= f.password_field :password, autocomplete: "off" %>
      </p>

      <p>
        <%= f.submit "Log In"%>
      </p>

    <% end %>
    <br>

  </body>
</html>
